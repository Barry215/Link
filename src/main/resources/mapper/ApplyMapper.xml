<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.SkyShadow.dao.ApplyMapper">
    <resultMap id="ModifyOrganization" type="cn.SkyShadow.model.apply.applyChildren.ModifyOrganization">
        <id column="" property="id" jdbcType="BIGINT"/>
        <association property="user" column="" select="cn.SkyShadow.dao.UserMapper.selectBaseInfo"/>
        <association property="organization" column="" select="cn.SkyShadow.dao.OrganizationMapper.selectByPrimaryKeyTemp"/>
    </resultMap>
    <insert id="createModifyOrganization" parameterType="cn.SkyShadow.model.apply.applyChildren.ModifyOrganization" keyProperty="orgId">
        INSERT INTO apply(APPLY_TYPE,USER,ORG_TEMP) VALUES('ModifyOrganization',#{user.userId,jdbcType=BIGINT},#{organization.orgId});
    </insert>
</mapper>